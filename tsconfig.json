{
	"compilerOptions": {
		"target": "ES2018",
		"module": "commonjs",
		"moduleResolution": "node",
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"outDir": "./dist",
		"strict": true,
		"noEmit": true,
		"lib": ["es2021"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"ts-node": {
		"swc": true
	}
}
